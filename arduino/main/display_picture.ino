#include "epd.h"



const int led = 13;                           //user led

const char *map_data = "";

void draw_map(void) {
  int count = 0;
  epd_set_color(BLACK, WHITE);
  for (int j = 0; j < 200; j++)
  {
    for (int i = 0; i < 300; i++)
    {
      if (map_data[i * 200 + j] == '1')
      {
        epd_fill_rect(i * 2, (200 - j) * 2, i * 2 + 1, (200 - j) * 2 + 1);
        count++;
      }
    }
  }
  epd_udpate();
  delay(15000);
}

