void draw_setup(void)
{
  /*
  user led init
  */

  pinMode(led, OUTPUT);
  digitalWrite(led, LOW);
  const int wake_up = 5;
  const int reset = 4;
  
  epd_init(wake_up, reset);
  epd_wakeup(wake_up);
  epd_set_memory(MEM_NAND);
}

